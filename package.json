{
    "name": "my-discord-bot",
    "version": "1.0.0",
    "description": "A Discord bot with an application dashboard",
    "main": "src/server.ts",
    "scripts": {
        "start": "ts-node src/server.ts",
        "dev": "concurrently \"ts-node src/server.ts\" \"npm run dev --prefix src/frontend\"",
        "build:frontend": "cd src/frontend && npm install && npm run build"
    },
    "dependencies": {
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "discord.js": "^14.3.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/node": "^16.11.7",
        "ts-node": "^10.4.0",
        "typescript": "^4.4.4",
        "concurrently": "^6.3.0"
    }
}
